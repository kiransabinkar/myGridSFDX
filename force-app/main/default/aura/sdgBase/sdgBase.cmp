<aura:component abstract="true">
    <!-- Abstract base component -->
    <aura:attribute name="DebugLog" type="String[]" default="" />
    <aura:attribute name="ShowDebug" type="Boolean" default="false" />
    <aura:handler name="init" value="{!this}" action="{!c.doBaseInit}" />

    <div aura:id="placeholder">
        {!v.body}
    </div>



</aura:component>